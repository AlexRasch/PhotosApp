@using PhotosApp.Views.Photos
@model IndexVM[]

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>API Test</title>
    
</head>
<body>
    <ul>
        @foreach (var item in Model)
        {
            <li>@item.Title</li>
            <li><a href="@item.Url"><img src="@item.ThumbnailUrl" /></a></li>
           
        }
    </ul>
</body>
</html>